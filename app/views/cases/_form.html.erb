<%= simple_form_for(@case) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :number %>
    <%= f.input :description %>
    <%= f.input :medical_bills %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
