<div class="case-header">
      <div class="case-title">
          <div style="font-size: 24px;">
                  <strong>Time Entries</strong><br>
          </div>
              <div id="case-buttons">
                  <ul>
                        <li><%= render 'new_button' %></li>
                  </ul>
            </div>
      </div>
      <strong style="color: white;">A</strong>
    <div class="container-fluid" style="padding: 0;">
        <div class="row">
            <div class="col-md-12">
                  <%= render partial: 'time_entries_tabs' %>
            </div>
        </div>
    </div>
</div>
        <div class="incase-tabs-gap"></div>
              <div style="margin-top: 70px;">
                    <h3>Time Entries</h3>
              </div>
<div class="table-responsive">
  <table id="tasks" class="table table-striped table-hover rowlink time_entries_table">
    <thead class="user-table-head">
    <tr>
      <th>Timekeeper</th>
      <th>Date</th>
      <th>Case</th>
      <th>Description</th>
      <th>Hours</th>
    </tr>
    </thead>
    <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr data-id="<%= time_entry.id %>">
        <td  style="vertical-align: middle;"><%= link_to time_entry.user.name, time_entry_path(time_entry) %></td>
        <td style="vertical-align: middle;"><%= time_entry.date %></td>
        <td class="mid-td"><div class="larger-td" style='padding-top:12px;'><%= time_entry.case.try(:name) %></div></td>
        <td style="vertical-align: middle;"><%= time_entry.description %></td>
        <td style="vertical-align: middle;"><%= time_entry.hours %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= render :partial => "index_javascript" %>


