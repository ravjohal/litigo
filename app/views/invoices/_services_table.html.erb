<% @services.each do |expense| %>
  <tr>
    <td><%= check_box_tag 'selected_service[]', expense.id, @ids.to_a.include?(expense.id.to_s) %></td>
    <td><%= expense.user.try(:name) %></td>
    <td><%= expense.activity %></td>
    <td><%= expense.html_hours %></td>
    <td><%= expense.html_rate %></td>
    <td><%= number_to_currency expense.html_amount %></td>
  </tr>
<% end %>