<%= simple_form_for(@case) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :number, :label => "Case number" %>
    <%= f.association :case_type, :label => "Case Type" %>
    <%= f.association :subtype, :label => "Sub-type" %>
    <%= f.input :description %>
    <%= f.input :medical_bills, :label => "Medical Bills" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>