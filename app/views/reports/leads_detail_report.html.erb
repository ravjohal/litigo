<%= form_tag reports_leads_detail_path, method: :get do %>
	<%= date_field_tag :start_date, @start_date %>
	<%= date_field_tag :end_date, @end_date %>
	<%= text_field_tag :marketing_channel_arg, @marketing_channel_arg %>
	<%= submit_tag "Run", name: nil %>
<% end %>
<div class="span6">
  <%= render template: 'dossier/reports/show', locals: {report: @leads_detail_report.run} %>
</div>